CREATE table "TB_CURSO" (
  "ID" INTEGER NOT NULL,
  "NOME" TEXT NOT NULL,
  primary key("ID" AUTOINCREMENT)
);

CREATE TABLE "TB_PROFESSOR"(
  "ID" INTEGER NOT NULL,
  "NOME" TEXT NOT NULL,
  "CURSO_ID" INTEGER NOT NULL,
  foreign key("CURSO_ID") REFERENCES "TB_CURSO"("ID")
);

CREATE TABLE "TB_CURSO_PROFESSOR" (
  "ID" INTEGER NOT NULL,
  "CURSO_ID" INTEGER NOT NULL,
  "PROFESSOR_ID" TEXT NOT NULL,
  foreign key("CURSO_ID") references "TB_CURSO"("ID"),
  primary key("ID" AUTOINCREMENT)
);

